{
  "name": "svg-icon-baker",
  "version": "0.1.0-beta.1",
  "description": "A delightful toolkit for baking raw SVG icons into delicious SVG symbol sprites",
  "type": "module",
  "license": "MIT",
  "author": "yangxu52",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stup",
    "test": "vitest run",
    "test:watch": "vitest",
    "log": "conventional-changelog -p angular -i ../../CHANGELOG.md"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "keywords": [
    "svg",
    "icon",
    "symbol",
    "sprite",
    "optimize",
    "transform"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangxu52/svg-icon-baker.git"
  },
  "bugs": {
    "url": "https://github.com/yangxu52/svg-icon-baker/issues"
  },
  "dependencies": {
    "svgo": "^4.0.0"
  },
  "devDependencies": {
    "unbuild": "^3.6.1",
    "vitest": "^4.0.4"
  }
}
