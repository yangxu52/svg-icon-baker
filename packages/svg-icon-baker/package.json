{
  "name": "svg-icon-baker",
  "version": "1.0.0-rc.1",
  "description": "A delightful toolkit for baking raw SVG icons into delicious SVG symbol sprites",
  "keywords": [
    "svg",
    "icon",
    "symbol",
    "sprite",
    "optimize",
    "transform",
    "convert"
  ],
  "homepage": "https://github.com/yangxu52/svg-icon-baker",
  "bugs": {
    "url": "https://github.com/yangxu52/svg-icon-baker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangxu52/svg-icon-baker.git"
  },
  "license": "MIT",
  "author": "yangxu52",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "unbuild --minify",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "svgo": "^4.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.8",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || >=24.0.0",
    "pnpm": ">=10.0.0"
  }
